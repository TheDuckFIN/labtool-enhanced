<%= render 'courses/navigation' %>

<h3>Editing week <%= @week.number %></h3>

<%= form_for @week do |f| %>
  <div class="form-group">
    <%= f.label :max_points %>
    <%= f.number_field :max_points, class:"form-control" %>
  </div>

  <div class="form-group">
    <%= f.submit class:"btn btn-success" %>
  </div>
<% end %>

